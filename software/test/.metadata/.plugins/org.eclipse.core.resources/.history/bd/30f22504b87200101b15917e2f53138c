#ifndef MOTOR_H_
#define MOTOR_H_

#include "stm32f407xx.h"
#include <ctype.h>

#include "button_brake.h"
#include "button_run.h"

struct motor {

	/* Set duty cycle 0-100 */
	uint8_t _dutyCycle;

	/* BRAKE button */
	struct button_brake _brake;

	/* RUN button */
	struct button_brake _run;

};




#endif /* MOTOR_H_ */
