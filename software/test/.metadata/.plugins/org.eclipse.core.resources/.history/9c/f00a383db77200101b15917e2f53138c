#ifndef CONTROL_H_
#define CONTROL_H_

#include "stm32f407xx.h"
#include <ctype.h>

#include "button_brake.h"
#include "button_run.h"

struct button_control {

	/* Set duty cycle */
	uint8_t _dutyCycle;

	/* BRAKE */
	struct button_brake _brake;

	/* RUN */
	struct button_brake _run;

};


#endif /* CONTROL_H_ */
